<odoo>
    <data>

        <record id="siswa_sd_ocb11.wizard_naik_kelas" model="ir.ui.view">
            <field name="name">wizard.ocb11.wizard_naik_kelas</field>
            <field name="model">siswa_sd_ocb11.wizard_naik_kelas</field>
            <field name="arch" type="xml">
                <form string="Kenaikan Kelas" class="o_sale_order" create="false">
                    <sheet>
                        <group>
                            <group>
                                <field name="tahunajaran_id" context="{'active_test': False}" options="{'no_create':True, 'no_edit':True, 'no_create_edit': True, 'no_open':True}"/>
                                <field name="jenjang_id" options="{'no_create':True, 'no_edit':True, 'no_create_edit': True, 'no_open':True}" />  
                            </group>
                            <group>
                                <field name="next_tahunajaran_id" context="{'active_test': False}" options="{'no_create':True, 'no_edit':True, 'no_create_edit': True, 'no_open':True}"/>
                                <field name="next_jenjang_id" options="{'no_create':True, 'no_edit':True, 'no_create_edit': True, 'no_open':True}" />
                            </group>
                        </group>
                    </sheet>
                    <!-- <footer class="oe_edit_only" >
                        <button name="action_pindah_kelas" type="object"
                            string="Save" class="oe_highlight"/>
                            
                        <button special="cancel" string="Cancel"/>
                    </footer> -->
                </form>
            </field>
        </record>

        <!-- <act_window id="siswa_sd_ocb11.wizard_naik_kelas_action"
            name="Kenaikan Kelas"
            src_model="res.partner"
            res_model="siswa_sd_ocb11.wizard_naik_kelas"
            view_mode="form"
            target="new"
            key2="client_action_multi"/> -->

        <record model="ir.actions.act_window" id="siswa_sd_ocb11.wizad_kenaikan_kelas_action">
            <field name="name">Kenaikan Kelas</field>
            <field name="res_model">siswa_sd_ocb11.wizard_naik_kelas</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>